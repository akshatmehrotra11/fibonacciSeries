import java.util.Scanner;
public class fibonacci {
	public static void main(String []args)
	{
	Scanner sc=new Scanner(System.in);
	System.out.println("Enter the no of terms till u want fibonacci to print");
	int term =sc.nextInt();
	
	
  int n1=0,n2=1,n3,i;
  
  System.out.print(n1 + " " + n2);
  
  for(i=2;i<term;i++)
  {
	  n3=n1+n2;
	  System.out.print(" "+ n3);
	  n1=n2;
	  n2=n3;
  }
}
}
